@model IEnumerable<Employee_Microservice.Models.Employee>

@{
    ViewData["Title"] = "Index";
}

<h1>Employee Details</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EmpName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.Passcode)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.WorkLocation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContactNumber)
            </th>
           @* <th>
                @Html.DisplayNameFor(model => model.Adminflag)
            </th>*@
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EmpName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
           @* <td>
                @Html.DisplayFor(modelItem => item.Passcode)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.WorkLocation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContactNumber)
            </td>
           @* <td>
                @Html.DisplayFor(modelItem => item.Adminflag)
            </td>*@
            <td>
                <a asp-action="Edit" asp-route-id="@item.EmpId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EmpId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EmpId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
